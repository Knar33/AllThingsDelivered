@model AllThingsDelivered.Models.CustomCartIndex
@{
    ViewBag.Title = "Index";
}
@if (ViewBag.Message != null)
{
    <div class="alert alert-success">
        @ViewBag.Message
    </div>
}
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-group">
        @Html.LabelFor(m => m.itemLocation, new { @class = "control-label" })
        @Html.TextBoxFor(m => m.itemLocation, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.itemLocation)
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.content, new { @class = "control-label" })
        @Html.TextBoxFor(m => m.content, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.content)
    </div>
    <input type="submit" />
}